<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>Logs of User</title>
  <link rel="stylesheet" href="/public/css/todolist.css">
  <%- include('../templates/head'); -%>
</head>

<body onload="startTime()">
  <div class="bg-dark">
    <%- include('../templates/header'); -%>
  </div>
  <div class="todolist ">
    <br>
    <div class="container mt-5 mb-5 d-flex justify-content-center">
      <div class="row">
        <div class="box" id="heading">
          <h1><%= ListTitle%></h1>
        </div>
        <div class="mt-5"></div>
        <% for(var i=0;i<newListItems.length;i++){ %>
          <div class="item col-md-1 box d-flex justify-content-center">
            <p><%= ((pageNo-1)*(size))+i+1 %></p>
          </div>
        <div class="item col-md-2 box d-flex justify-content-center">
          <p> <%= newListItems[i].authtype %> </p>
        </div>
        <div class="item col-md-9 box d-flex justify-content-center text-center">
          <p> <%= newListItems[i].time %> </p>
        </div>
        <% } %>
      </div>
    </div>
    <div class="container d-md-flex justify-content-center text-center">
      <% for(var i=1;i<=maxlist;i++){ %>
        <a class="box mx-2 px-2" href="/account/logs?page=<%= i %>&limit=15"><%= i %></a>
      <% } %>
    </div>
    <br>
  </div>
  <div class="bg-dark">
    <%- include('../templates/footer'); -%>
  </div>

</body>

</html>
